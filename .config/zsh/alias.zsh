# Colored dmesg output
alias dmesg='dmesg --color=always'
alias grep='grep --color=auto'

# List directory contents
alias ls='ls --color=auto'
alias lsa='ls -lah'
alias l='ls -lah'
alias ll='ls -lh'n
alias la='ls -lAh'

# directory stack nav
alias -- -='cd -'
alias 1='cd +1'
alias 2='cd +2'
alias 3='cd +3'
alias 4='cd +4'
alias 5='cd +5'
alias 6='cd +6'
alias 7='cd +7'
alias 8='cd +8'
alias 9='cd +9'

alias md='mkdir -p'
alias rd=rmdir

alias help=run-help